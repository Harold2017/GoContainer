reset;

set terminal dumb size 120, 60; set autoscale;

set grid;

set lmargin 10;
set rmargin 10;
set tmargin 10;
set bmargin 10;

set title 'Benchmark on heap / bst / hashmap';

set xlabel 'Tests';
set ylabel 'Size';

# key/legend
set key top left;
# set key left bottom;
set key box;
# set key bmargin;

plot "res.dat" using 2 title 'heap' with lines lt 1, \
     "res.dat" using 3 title 'red-black-tree' with lines lt 2, \
     "res.dat" using 4 title 'b-tree' with lines lt 3, \
     "res.dat" using 5 title 'graph' with lines lt 4